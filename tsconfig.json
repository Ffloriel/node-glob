    "declarationMap": true,
    "inlineSources": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "target": "es2022",
    "outDir": "deleteme"
  }
}
